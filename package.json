{
  "name": "hex-management",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "echo STRIPE_WEBHOOK_SECRET=$(stripe listen --print-secret) > .env.local && stripe listen --forward-to localhost:3000/api/auth/stripe/webhook & next dev",
    "dev-no-turbopack": "next dev ",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "prisma:dev": "prisma migrate dev --preview-feature && prisma generate",
    "prisma:deploy": "prisma migrate deploy && prisma generate",
    "prisma:studio": "prisma studio",
    "prisma:reset": "prisma migrate reset --force && prisma generate",
    "prisma:status": "prisma migrate status",
    "prisma:format": "prisma format",
    "prisma:sync": "if [ \"$NODE_ENV\" = \"production\" ]; then npm run prisma:deploy; else npm run prisma:dev; fi"
  },
  "dependencies": {
